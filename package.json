{
  "name": "jqf",
  "version": "1.5.2",
  "description": "Jqf is a CLI JSON processor with JavaScript function syntax",
  "keywords": [
    "json",
    "processor",
    "javascript",
    "CLI"
  ],
  "main": "dist/index.js",
  "bin": "dist/cli.js",
  "license": "MIT",
  "repository": "git@github.com:kamataryo/jqf.git",
  "author": "kamataryo <mugil.cephalus+github.com@gmail.com>",
  "private": false,
  "scripts": {
    "build": "rimraf ./dist && tsc && chmod +x ./dist/cli.js",
    "lint": "eslint --cache --ext=ts ./src",
    "test": "npm run build && ava"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "devDependencies": {
    "@types/node": "^12.6.1",
    "@typescript-eslint/parser": "^1.11.0",
    "ava": "^2.3.0",
    "eslint": "^6.0.1",
    "rimraf": "^2.6.3",
    "ts-node": "^8.3.0",
    "typescript": "3.5.3"
  },
  "dependencies": {
    "@kamataryo/outdent": "^1.0.4",
    "chalk": "^4.0.0",
    "commander": "^5.0.0",
    "safe-eval": "^0.4.1"
  }
}
