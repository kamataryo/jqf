{
  "name": "jqf",
  "version": "1.7.0",
  "description": "Jqf is a CLI JSON processor with JavaScript function syntax",
  "keywords": [
    "json",
    "processor",
    "javascript",
    "CLI"
  ],
  "type": "module",
  "main": "dist/index.js",
  "bin": "dist/cli.js",
  "license": "MIT",
  "repository": "git@github.com:kamataryo/jqf.git",
  "author": "kamataryo <mugil.cephalus+github.com@gmail.com>",
  "private": false,
  "scripts": {
    "build": "bun build ./src/index.ts ./src/cli.ts --outdir ./dist --target node",
    "test": "npm run build && ava"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "devDependencies": {
    "@types/bun": "^1.0.8",
    "@types/node": "16"
  },
  "dependencies": {
    "@kamataryo/outdent": "^1.0.4",
    "commander": "^12.0.0",
    "picocolors": "^1.0.0",
    "safe-eval": "^0.4.1"
  }
}
